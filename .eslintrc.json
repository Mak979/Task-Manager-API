{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "no-empty": 0,
        "no-irregular-whitespace": 1,
        "linebreak-style": 0,
        "array-bracket-spacing": 1,
        "no-trailing-spaces": 2
    }
}
